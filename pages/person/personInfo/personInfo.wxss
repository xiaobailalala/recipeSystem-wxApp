page {
    height: 100%;
    background-color: #ffffff;
    position: relative;
}

.clear {
    clear: both;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

button::after {
    border: unset;
}

.headContainer {
    height: 450rpx;
    position: relative;
}

.head_bg {
    width: 100%;
    height: 100%;
    position: absolute;
    z-index: 0;
}

.head_bg_shadow {
    position: absolute;
    width: 100%;
    height: 100%;
    box-shadow: 0 30rpx 80rpx rgba(0, 0, 0, 0.3) inset;
    background: linear-gradient(to top, rgba(0, 0, 0, 0.5) 10%, rgba(0, 0, 0, 0) 90%);
    z-index: 1;
}

.head_bg_baseInfo {
    height: 100rpx;
    position: absolute;
    width: 100%;
    bottom: 60rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-around;
}

.head_bg_baseInfo>view:nth-of-type(1) {
    color: #ffffff;
    font-size: 36rpx;
}

.head_bg_baseInfo>view:nth-of-type(2) {
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 26rpx;
    color: #ffffff;
    width: 360rpx;
}

.head_userHead {
    display: inline-block;
    position: absolute;
    bottom: 190rpx;
    left: 50%;
    margin-left: -70rpx;
    width: 140rpx;
    height: 140rpx;
    border-radius: 50%;
    box-sizing: border-box;
    border: 6rpx solid #ffffff;
    overflow: hidden;
    z-index: 10;
}

.head_userHead>image {
    width: 100%;
    height: 100%;
}

.head_tools {
    position: absolute;
    z-index: 10;
    color: #ffffff;
    right: 15rpx;
    top: 7rpx;
    font-weight: bold;
}

.head_tools>icon {
    margin-left: 30rpx;
    font-size: 36rpx;
    vertical-align: top;
}

.shareBtn{
    display: inline-block;
    padding: 0;
    margin: 0;
    background-color: unset;
    vertical-align: top;
    line-height: unset;
}

.shareBtn>icon{
    margin-left: 30rpx;
    font-size: 36rpx;
    color: #ffffff;
    font-weight: bold;
}

.collect_count {
    display: flex;
    align-items: center;
    justify-content: space-around;
    padding: 50rpx 0;
}

.collect_count>navigator>view:nth-of-type(1) {
    text-align: center;
    font-size: 54rpx;
}

.collect_count>navigator>view:nth-of-type(2) {
    font-size: 24rpx;
    color: #888888;
}

.collect_sign {
    padding: 0 60rpx;
    color: #666666;
    font-size: 30rpx;
    line-height: 1.5;
}

.content_head {
    border-top: 1rpx solid #e5e3df;
    border-bottom: 1rpx solid #e5e5e5;
    margin-top: 40rpx;
    height: 80rpx;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    background-color: #ffffff;
}

.content_head_height {
    height: 120rpx;
    width: 100%;
    z-index: 20;
}

.content_head_fixed {
    position: fixed;
    top: 0;
    margin-top: 0;
}

.content_head>view {
    box-sizing: border-box;
    height: 100%;
    line-height: 80rpx;
    margin: 0 80rpx;
    font-size: 28rpx;
    font-weight: bold;
}

.content_head_on {
    border-bottom: 4rpx solid #000000;
}

.content_main {
    padding-top: 30rpx;
}

.noneTips {
    text-align: center;
    font-size: 30rpx;
    color: #888888;
}

.content_recipe,
.content_article {
    padding: 0 50rpx;
}

.content_recipe>view {
    margin-bottom: 50rpx;
}

.content_recipe>view>navigator:nth-of-type(1) {
    font-size: 31rpx;
    font-weight: bold;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.content_recipe>view>view>icon {
    font-size: 22rpx;
    color: #888888;
    margin-right: 40rpx;
}

.content_recipe>view>navigator:nth-of-type(2) {
    width: 100%;
    height: 360rpx;
    border-radius: 10rpx;
    overflow: hidden;
    margin-top: 10rpx;
}

.content_recipe>view>navigator:nth-of-type(2)>image {
    width: 100%;
    height: 100%;
}

.likeListMain {
    display: flex;
    align-items: flex-start;
    justify-content: space-between;
}

.likeListMain>view {
    width: 47.2%;
    background-color: #ffffff;
}

.likeListMain>view>navigator {
    width: 100%;
    border-radius: 10rpx;
    box-shadow: 0 0 16rpx rgb(180, 180, 180);
    overflow: hidden;
    margin-bottom: 30rpx;
    padding-bottom: 20rpx;
}

.likeListMain>view>navigator>image {
    width: 100%;
    vertical-align: top;
}

.likeListMain>view>navigator>view:nth-of-type(1) {
    font-size: 26rpx;
    padding: 12rpx 16rpx 0 16rpx;
    overflow: hidden !important;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    max-height: 104rpx;
    box-sizing: border-box;
}

.likeInfo {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-top: 16rpx;
    padding: 0 16rpx;
    font-size: 20rpx;
}

.likeInfo>view {
    display: flex;
    align-items: center;
    color: #888888;
}

.likeInfo>view>image {
    width: 40rpx;
    height: 40rpx;
    border-radius: 50%;
    margin-right: 10rpx;
}

.likeInfo>view>view {
    max-width: 150rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.likeInfo>icon {
    font-size: 20rpx;
    position: relative;
    top: -13rpx;
    color: #888888;
}