page {
    height: 100%;
    background-color: #ffffff;
    position: relative;
    overflow: hidden;
}

.clear {
    clear: both;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

button::after {
    border: unset;
}

.bg {
    width: 100%;
    position: absolute;
    bottom: 0;
    left: 0;
    z-index: -1;
}

.search_head {
    border-bottom: 1rpx solid #f2f1f0;
    height: 80rpx;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 50rpx;
    box-sizing: border-box;
    background-color: #ffffff;
}

.search_head>view {
    display: flex;
    align-items: center;
    justify-content: space-between;
    background-color: #f7f7f7;
    border-radius: 10rpx;
    width: 100%;
    padding-left: 18rpx;
}

.search_head>view>icon {
    color: #b7b7b7;
    position: relative;
    top: -8rpx;
    width: 8%;
}

.search_head>view>input {
    width: 92%;
    height: 100%;
    font-size: 28rpx;
}

.search_head>button{
    background-color: #ffffff;
    color: #888888;
    line-height: unset;
    margin: 0;
    padding: 0 15rpx;
    border: 2rpx solid #d8d8d8;
    border-radius: 50%;
    margin-right: 30rpx;
}

.search_head>button>icon{
    position: relative;
    top: -6rpx;
    left: -2rpx;
}

.materialContainer{
    display: flex;
    align-items: flex-start;
    justify-content: space-between;
    flex-wrap: wrap;
}

.materialContainer>navigator{
    box-sizing: border-box;
    border-right: 1rpx solid #e2e2e2;
    border-bottom: 1rpx solid #e2e2e2;
    display: flex;
    align-items: center;
    justify-content: space-between;
    flex-direction: column;
    width: 25%;
    padding: 20rpx 0;
    font-size: 30rpx;
    color: #888888;
}

.materialContainer>navigator:nth-of-type(4n) {
    border-right: 0;
}

.materialContainer>navigator>image{
    width: 100rpx;
    height: 100rpx;
    border-radius: 40%;
    margin-top: 20rpx;
}

.searchListContainer navigator{
    display: flex;
    align-items: center;
    justify-content: flex-start;
    padding: 20rpx 50rpx;
    border-bottom: 1rpx solid #cbcbcb;
    position: relative;
}

.searchListContainer navigator>image{
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    margin-right: 50rpx;
}

.searchListContainer navigator>icon{
    position: absolute;
    right: 50rpx;
    font-size: 40rpx;
    font-weight: bold;
    color: #999999;
    top: 50%;
    margin-top: -30rpx;
}