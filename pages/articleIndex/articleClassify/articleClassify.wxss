page {
    height: 100%;
    position: relative;
    background-color: #ffffff;
}

.bg {
    width: 100%;
    height: 100%;
    position: fixed;
    z-index: 0;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

button::after {
    border: unset;
}

.headContainer {
    border-bottom: 24rpx solid #f7f7f7;
    text-align: center;
    padding: 45rpx 0;
}

.headImg {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    font-weight: bold;
}

.headImg>image {
    width: 180rpx;
    height: 180rpx;
    border-radius: 10rpx;
    margin-bottom: 26rpx;
}

.headPeople{
    font-size: 26rpx;
    color: #ffbb32;
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-top: 30rpx;
}

.headPeople>view{
    width: 50%;
    box-sizing: border-box;
}

.headPeople>view:nth-of-type(1){
    text-align: right;
    padding-right: 12rpx;
}

.headPeople>view:nth-of-type(2){
    padding-left: 12rpx;
    text-align: left;
    display: flex;
    align-items: center;
    justify-content: flex-start;
    position: relative;
}

.headPeople>view>image{
    width: 50rpx;
    height: 50rpx;
    border-radius: 50%;
    border: 4rpx solid #ffffff;
    vertical-align: top;
    position: absolute;
}

.headPeople>view>image:nth-of-type(2){
    left: 50rpx;
}

.headPeople>view>image:nth-of-type(3){
    left: 90rpx;
}

.headPeople>view>image:nth-of-type(4){
    left: 130rpx;
}

.headPeople>view>image:nth-of-type(5){
    left: 170rpx;
}

.headPeople>view>image:nth-of-type(6){
    left: 210rpx;
}

.contentContainer{
    padding: 0 40rpx;
}

.newest {
    text-align: center;
    font-weight: bold;
    padding: 32rpx 0;
}

.likeListMain {
    display: flex;
    align-items: flex-start;
    justify-content: space-between;
}

.likeListMain>view {
    width: 47.2%;
    background-color: #ffffff;
}

.likeListMain>view>navigator {
    width: 100%;
    border-radius: 10rpx;
    box-shadow: 0 0 16rpx rgb(180, 180, 180);
    overflow: hidden;
    margin-bottom: 30rpx;
    padding-bottom: 20rpx;
}

.likeListMain>view>navigator>image {
    width: 100%;
    vertical-align: top;
}

.likeListMain>view>navigator>view:nth-of-type(1) {
    font-size: 26rpx;
    padding: 12rpx 16rpx 0 16rpx;
    overflow: hidden !important;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    max-height: 104rpx;
    box-sizing: border-box;
}

.likeInfo {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-top: 16rpx;
    padding: 0 16rpx;
    font-size: 20rpx;
}

.likeInfo>view {
    display: flex;
    align-items: center;
    color: #888888;
}

.likeInfo>view>image {
    width: 40rpx;
    height: 40rpx;
    border-radius: 50%;
    margin-right: 10rpx;
}

.likeInfo>view>view {
    max-width: 150rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.likeInfo>icon {
    font-size: 20rpx;
    position: relative;
    top: -13rpx;
    color: #888888;
}

.fixedWriteTag{
    position: fixed;
    z-index: 10;
    bottom: 60rpx;
    background-color: #333333;
    color: #f8af1b;
    font-weight: bold;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 16rpx 50rpx;
    font-size: 28rpx;
    border-radius: 42rpx;
    left: 50%;
    margin-left: -113rpx;
    box-shadow: 0 0 30rpx #333333;
}

.fixedWriteTag>icon{
    margin-right: 16rpx;
    position: relative;
    top: -8rpx;
    font-size: 26rpx;
}